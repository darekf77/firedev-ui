{
    "manifest_version": 3,
    "name": "Firedev Websql",
    "description": "Manage your websql databases in firedev.io framework",
    "version": "1.0",
    "action": {
      "default_popup": "start.html",
      "default_icon": "icon.png"
    },
    "devtools_page": "devtools.html",
    "permissions": [
      "tabs",
      "http://*/*",
      "https://*/*"
    ]
  }